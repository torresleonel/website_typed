<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <template id="assets_frontend" inherit_id="web.assets_frontend" name="TypedJS Website Assets">
    <xpath expr="//script[last()]" position="after">
      <script type="text/javascript" src="/website_typed/static/lib/typedjs/typed.js"/>
      <script type="text/javascript" src="/website_typed/static/src/js/content/typed.snippet.animation.js"/>
    </xpath>
  </template>

  <template id="assets_editor" inherit_id="website.assets_editor" name="TypedJS Website Editor Assets">
    <xpath expr="//script[last()]" position="after">
      <script type="text/javascript" src="/website_typed/static/src/js/editor/typed.snippet.options.js"/>
    </xpath>
  </template>
</odoo>